@prefix world: <http://example.com/data/world/> .
@prefix worldOnt: <http://example.com/schema/worldOntology#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix dc:   <http://purl.org/dc/elements/1.1/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

world:rome a worldOnt:City ;
    rdfs:label "Rome"@en ;
  	worldOnt:name "Rome"^^xsd:string .

world:scipio a worldOnt:Person ;
	rdfs:label "Africanus"@en ;
    worldOnt:name "Scipio"^^xsd:string ;
    worldOnt:address "3 palace street, Rome"@en .

world:carthage a worldOnt:City ;
    rdfs:label "Carthage"@en ;
    worldOnt:name "Carthage"^^xsd:string .

world:hannibal a worldOnt:Person ;
	rdfs:label "Hannibal"@en ;
    worldOnt:name "Hannibal"^^xsd:string ;
    worldOnt:address "45 temple road, Carthage"@en .

world:scipioJr a worldOnt:Person ;
	rdfs:label "Africanus Junior"@en ;
    worldOnt:name "Scipio Junior"^^xsd:string ;
    worldOnt:address "3 palace street, Rome"@en .

world:scipioJrs a worldOnt:Person ;
    rdfs:label "Africanus Junioress"@en ;
    worldOnt:address "3 palace street, Rome"@en ;
    worldOnt:name "Scipio Junioress"^^xsd:string ;
    worldOnt:birthday "-0228-10-10T00:00:00Z"^^xsd:dateTime.
